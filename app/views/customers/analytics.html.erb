<table class="header-table" width="90%">
  <tr>
    <th colspan="8">ステータス別結果</th>
  </tr>
  <tr>
    <th>不在</th>
    <th>担当者不在</th>
    <th>見込</th>
    <th>折返待</th>
    <th>APP</th>
    <th>今は結構</th>
    <th>外国人NG</th>
    <th>フロントNG</th>
  </tr>
  <tr>
    <td><%= @call_count.where(statu: "不在").count %></td>
    <td><%= @call_count.where(statu: "担当者不在").count %></td>
    <td><%= @call_count.where(statu: "見込").count %></td>
    <td><%= @call_count.where(statu: "折返待").count %></td>
    <td><%= @call_count.where(statu: "APP").count %></td>
    <td><%= @call_count.where(statu: "今は結構").count %></td>
    <td><%= @call_count.where(statu: "外国人NG").count %></td>
    <td><%= @call_count.where(statu: "フロントNG").count %></td>
  </tr>
  <tr>
    <td><%= number_to_percentage(@call_count.where(statu: "不在").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "担当者不在").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "見込").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "折返待").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "APP").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "今は結構").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "外国人NG").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
    <td><%= number_to_percentage(@call_count.where(statu: "フロントNG").count / @call_count.count.to_f * 100, precision: 1 ) %></td>
  </tr>
</table>

