    <div class="heading"><h2>ドキュメントダウンロード</h2>
      <%= link_to '（アップロードはこちら）', action: "form" %>
            </div>

    <table width="90%" data-turbolinks="false">
     <tbody>
          <% UploadFile.all.reverse_order.each do |upload| %>
        <tr>
          <th>ファイル名</th>
          <th>ダウンロード</th>
          <th>編集/削除</th>
        </tr>
        <tr>
            <th><%= link_to upload.name, {action: "view", id: upload.id} %></th>
            <td><%= link_to "download", {action: "download", id: upload.id} %></td>
            <td><%= link_to '編集', edit_uploader_path(upload), class: 'command'%>
			            ｜ <%= link_to '削除',
			            	uploader_path(upload),
			            	method: :delete,
			            	class: 'command',
			            	data: { confirm: '本当に削除しますか？'} %></td>
        </tr>
      <% end %>
      </tbody>
    </table>

 </div>
